{
  "id": null,
  "title": "Mission Management System - Comprehensive Dashboard",
  "tags": ["mission", "monitoring", "analytics", "performance", "business"],
  "style": "dark",
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "System Health & Service Status",
      "type": "stat",
      "targets": [
        {
          "expr": "up",
          "legendFormat": "{{job}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {"color": "red", "value": 0},
              {"color": "green", "value": 1}
            ]
          },
          "mappings": [
            {
              "options": {
                "0": {"text": "DOWN", "color": "red"},
                "1": {"text": "UP", "color": "green"}
              },
              "type": "value"
            }
          ],
          "unit": "short"
        }
      },
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      }
    },
    {
      "id": 2,
      "title": "Mission Business Metrics",
      "type": "stat",
      "targets": [
        {
          "expr": "mission_created_total",
          "legendFormat": "Total Missions Created"
        },
        {
          "expr": "mission_active_count",
          "legendFormat": "Active Missions"
        },
        {
          "expr": "vehicle_utilization_rate",
          "legendFormat": "Vehicle Utilization %"
        },
        {
          "expr": "incident_reported_total",
          "legendFormat": "Incidents Reported"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "short",
          "thresholds": {
            "steps": [
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 10},
              {"color": "red", "value": 20}
            ]
          }
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      }
    },
    {
      "id": 3,
      "title": "HTTP Performance Analytics",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(http_request_duration_seconds_sum[5m]) / rate(http_request_duration_seconds_count[5m])",
          "legendFormat": "Avg Response Time"
        },
        {
          "expr": "rate(http_request_duration_seconds_count[5m])",
          "legendFormat": "Request Rate (req/s)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "barAlignment": 0,
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "none",
            "spanNulls": false
          },
          "unit": "s"
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "options": {
        "legend": {
          "calcs": ["mean", "max", "min"],
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 4,
      "title": "Container Resource Utilization",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(container_cpu_usage_seconds_total{container=~\"mission.*\"}[5m]) * 100",
          "legendFormat": "{{container}} CPU %"
        },
        {
          "expr": "container_memory_usage_bytes{container=~\"mission.*\"} / container_spec_memory_limit_bytes{container=~\"mission.*\"} * 100",
          "legendFormat": "{{container}} Memory %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "barAlignment": 0,
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "none",
            "spanNulls": false
          },
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "steps": [
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 70},
              {"color": "red", "value": 90}
            ]
          }
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 5,
      "title": "Real-time Mission Activity",
      "type": "piechart",
      "targets": [
        {
          "expr": "mission_active_count",
          "legendFormat": "Active Missions"
        },
        {
          "expr": "vehicle_utilization_rate",
          "legendFormat": "Vehicle Utilization"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            }
          }
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "values": ["percent", "value"]
        },
        "pieType": "pie",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        }
      }
    },
    {
      "id": 6,
      "title": "System Performance Trends",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
          "legendFormat": "95th Percentile Response Time"
        },
        {
          "expr": "rate(http_request_duration_seconds_count{code=~\"5..\"}[5m])",
          "legendFormat": "Error Rate (5xx)"
        },
        {
          "expr": "rate(http_request_duration_seconds_count{code=~\"4..\"}[5m])",
          "legendFormat": "Client Error Rate (4xx)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "barAlignment": 0,
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "none",
            "spanNulls": false
          },
          "unit": "s"
        }
      },
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24},
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "30s",
  "schemaVersion": 30,
  "version": 1,
  "links": [],
  "annotations": {
    "list": []
  },
  "templating": {
    "list": []
  }
}

